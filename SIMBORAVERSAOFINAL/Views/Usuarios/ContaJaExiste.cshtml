@model Simbora04.Models.Usuarios
@{
    ViewData["Title"] = "ContaJaExiste";
}
<head>

    <link rel="stylesheet" href="/css/ContaJaExisteStyle.css" />
</head>
<body>

    <div class="loginbox">

        <div class="col-md-4">
            <form asp-action="Create" class="center">
                <h1>Entrar</h1>
                <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group" style="width: 220px;">
                    <label asp-for="Nome" class="control-label"></label>
                    <input asp-for="Nome" class="form-control" />
                    <span asp-validation-for="Nome" class="text-danger"></span>
                </div>
                <div class="form-group" style="width: 220px;">
                    <label asp-for="Senha" class="control-label"></label>
                    <input asp-for="Senha" class="form-control" />
                    <span asp-validation-for="Senha" class="text-danger"></span>
                </div>
                <div class="form-group">
                    <a class="btn btn-primary" asp-controller="Eventos" asp-action="Index" style=" border: none; outline: none; height: 40px; background: #FF7575; color: #fff; font-size: 18px; border-radius: 20px; margin-top: 20px; width: 220px;">Entrar</a>
                         <a class="btn btn-primary" asp-action="Create" style="border: none; outline: none; height: 40px; background: #FF7575; color: #fff; font-size: 18px; border-radius: 20px; margin-top: 20px; width: 220px;">Voltar</a>
                </div>
            </form>

        </div>
    </div>
       
</body>